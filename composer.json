{
    "name": "irap/ec2-wrapper",
    "type": "library",
    "description": "Package for making it easier to work with AWS EC2.",
    "keywords": ["AWS","EC2"],
    "homepage": "http://www.irap.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "support@irap.org",
            "homepage": "https://github.com/iRAP-software/package-EC2-wrapper",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php" : "3.62.*",
        "ext-libxml" : "*",
        "ext-simplexml" : "*"
    },
    "require-dev": {
        "irap/autoloader": "^1.0",
        "irap/core-libs": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "iRAP\\Ec2Wrapper\\": "src/",
            "iRAP\\Ec2Wrapper\\Enums\\": "src/enums/",
            "iRAP\\Ec2Wrapper\\Objects\\": "src/objects/",
            "iRAP\\Ec2Wrapper\\Requests\\": "src/requests/",
            "iRAP\\Ec2Wrapper\\Responses\\": "src/responses/"
        }
    }
}
